{{!< default}}

{{> header}}

{{> navbar}}

<main class="content" role="main">
  <article  class="{{post_class}}">
    <div class="post-inner clearfix">
        <div class="column-left"></div>
        <div class="column-right">
          <header class="post-header">
            <h1 class="post-title">我的标签</h1>
          </header>
          <span class="post-meta clearfix"></span>

          <section class="post-content">
            <div class="tag-cloud">
              {{tag_cloud}}
            </div>
          </section>

          <footer class="post-footer">
            <section class="share bdsharebuttonbox">
              <div class="clearfix">
                <h4>立即分享：</h4>
                <a href="#" class="bds_tsina" data-cmd="tsina" title="分享到新浪微博"></a>
                <a href="#" class="bds_weixin" data-cmd="weixin" title="分享到微信"></a>
                <a href="#" class="bds_youdao" data-cmd="youdao" title="分享到有道云笔记"></a>
                <a href="#" class="bds_renren" data-cmd="renren" title="分享到人人网"></a>
                <a href="#" class="bds_douban" data-cmd="douban" title="分享到豆瓣网"></a>
                <a href="#" class="bds_more" data-cmd="more"></a>
              </div>
              <script>
              window._bd_share_config={
                "common":{"bdSnsKey":{},"bdText":"关于风影博客的标签-快去看看吧！","bdUrl":"{{@blog.url}}/tags/","bdMini":"2","bdMiniList":false,"bdPic":"","bdStyle":"0","bdSize":"24"},
                "share":{}
              };
              with(document)0[(getElementsByTagName('head')[0]||body).appendChild(createElement('script')).src='http://bdimg.share.baidu.com/static/api/js/share.js?v=89860593.js?cdnversion='+~(-new Date()/36e5)];
              </script>
            </section>
          </footer>
        </div>

        <br />

        {{!-- author --}}
        {{> author}}

        <br />
    </div>
  </article>

  <div class="to_top">
    <a href="#"><i class="fa fa-arrow-circle-up"></i></a>
  </div>
</main>